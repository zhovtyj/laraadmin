 {
    "name": "zhovtyj/laraadmin",
    "description": "LaraAdmin is a Open source Laravel Admin Panel / CMS which can be used as Admin Backend, Data Management Tool or CRM boilerplate for Laravel with features like CRUD Generation, Module Manager, Media, Menus, Backups and much more",
    "keywords": ["php", "laravel", "crm", "admin", "cms", "administrator"],
    "license": "MIT",
    "homepage": "http://laraadmin.com",
    "support": {
        "issues": "https://github.com/zhovtyjitsolutions/laraadmin/issues",
        "source": "https://github.com/zhovtyjitsolutions/laraadmin"
    },
    "version": "1.0",
    "require": {
        "illuminate/support": "5.*",
        "laravelcollective/html": "^5.6",
        "illuminate/container": "^5.6",
        "yajra/laravel-datatables-oracle": "~8.0",
        "creativeorange/gravatar": "^1.0",
        "doctrine/dbal": "~2.6",
        "zizaco/entrust": "1.7.0",
        "spatie/laravel-backup": "5.*"
    },
    "authors": [
        {
            "name": "Igor Zhovtaniuk",
            "email": "igorzhovtyj@gmail.com"
        }
    ],
    "classmap": [
        "src/Controllers",
        "src/Models"
    ],
    "autoload": {
        "psr-4": {
            "zhovtyj\\Laraadmin\\": "src/"
        },
        "files": [
        ]
    }
}
